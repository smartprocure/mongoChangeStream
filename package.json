{
  "name": "mongochangestream",
  "version": "0.17.0",
  "description": "Sync MongoDB collections via change streams into any database.",
  "author": "GovSpend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git://github.com/smartprocure/mongochangestream.git",
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc --declaration",
    "build:develop": "tsc --watch",
    "lint": "eslint src/**",
    "fmt": "prettier --ignore-path .gitignore --write './'"
  },
  "keywords": [
    "mongodb",
    "mongo",
    "change",
    "stream",
    "sync",
    "collection",
    "database",
    "datastore",
    "db",
    "elasticsearch",
    "sql"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "prom-utils": "^0.3.0"
  },
  "peerDependencies": {
    "ioredis": ">= 5.2.3",
    "mongodb": ">= 4.9.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
