{
  "name": "mongochangestream",
  "version": "0.38.0",
  "description": "Sync MongoDB collections via change streams into any database.",
  "author": "GovSpend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git://github.com/smartprocure/mongochangestream.git",
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "clean": "rm -rf dist",
    "build": "npm run fmt && npm run clean && tsc --declaration --project ./tsconfig-prod.json",
    "build:watch": "tsc --watch",
    "lint": "eslint src/**",
    "fmt": "prettier --ignore-path .gitignore --write './'"
  },
  "keywords": [
    "mongodb",
    "mongo",
    "change",
    "stream",
    "sync",
    "collection",
    "database",
    "datastore",
    "db",
    "elasticsearch",
    "sql",
    "cratedb",
    "health",
    "check"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "eslint": "^8.39.0",
    "ioredis": "5.3.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "eventemitter3": "^5.0.1",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "obj-walker": "^1.7.0",
    "prom-utils": "^0.4.0",
    "simple-machines": "^0.3.0"
  },
  "peerDependencies": {
    "ioredis": ">= 5.2.3",
    "mongodb": ">= 4.10.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
